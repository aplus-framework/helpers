{
	"name": "natanfelles/array-simple",
	"type": "library",
	"description": "The ArraySimple class contains methods that work with PHP arrays using \"simple keys\" (strings with brackets).",
	"keywords": [
		"array",
		"simple",
		"query",
		"brackets",
		"string"
	],
	"homepage": "https://github.com/natanfelles/array-simple",
	"license": "MIT",
	"authors": [
		{
			"name": "Natan Felles",
			"email": "natanfelles@gmail.com"
		}
	],
	"require": {
		"php": ">=7.3"
	},
	"require-dev": {
		"ext-xdebug": "*",
		"apigen/apigen": "dev-master#85290e8",
		"friendsofphp/php-cs-fixer": "^2.14",
		"localheinz/composer-normalize": "^1.1",
		"phpmd/phpmd": "^2.6",
		"phpstan/phpstan": "^0.11",
		"phpunit/phpunit": "^7.5"
	},
	"autoload": {
		"classmap": [
			"src/ArraySimple.php"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"support": {
		"issues": "https://github.com/natanfelles/array-simple/issues",
		"source": "https://github.com/natanfelles/array-simple"
	}
}
