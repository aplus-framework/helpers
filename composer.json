{
	"name": "natanfelles/helpers",
	"type": "library",
	"description": "The Framework Helpers Library",
	"keywords": [
		"array",
		"simple",
		"query",
		"square",
		"brackets",
		"string",
		"files"
	],
	"homepage": "https://the-framework.gitlab.io/libraries/helpers.html",
	"license": "MIT",
	"authors": [
		{
			"name": "Natan Felles",
			"email": "natanfelles@gmail.com",
			"homepage": "https://natanfelles.github.io"
		}
	],
	"require": {
		"php": ">=8.0"
	},
	"require-dev": {
		"ext-xdebug": "*",
		"ergebnis/composer-normalize": "^2.15",
		"jetbrains/phpstorm-attributes": "^1.0",
		"natanfelles/coding-standard": "^1.7",
		"phpmd/phpmd": "^2.10",
		"phpstan/phpstan": "^0.12",
		"phpunit/phpunit": "^9.5"
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"Framework\\Helpers\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Helpers\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"support": {
		"email": "natanfelles@gmail.com",
		"issues": "https://gitlab.com/the-framework/libraries/helpers/issues",
		"wiki": "https://gitlab.com/the-framework/libraries/helpers/wikis/home",
		"source": "https://gitlab.com/the-framework/libraries/helpers",
		"docs": "https://the-framework.gitlab.io/libraries/helpers/docs/"
	}
}
